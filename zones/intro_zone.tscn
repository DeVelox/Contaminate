[sub_resource type="GDScript" id="GDScript_vme7v"]
resource_name = "tutorial"
script/source = "extends Area2D

var mining_away: float = 1.0
var can_collect: bool

@onready var vignette := get_node(\"/root/Main/Shaders/Vignette\")
@onready var player := get_node(\"/root/Main/Player\")
# Called when the node enters the scene tree for the first time.
func _ready() -> void:
	vignette.material.set_shader_parameter(\"vignette_intensity\", 50.0)
	var tween := create_tween()
	tween.tween_property(vignette.material, \"shader_parameter/vignette_intensity\", 1.0, 1.0)


# Called every frame. 'delta' is the elapsed time since the previous frame.
func _process(delta: float) -> void:
	player.position.x = clamp(player.position.x, 0, 960)
	player.position.y = clamp(player.position.y, 0, 540)
	if can_collect: # and Input.is_action_pressed(\"shoot\"):
		mining_away -= 0.5 * delta
		$Sprite2D.self_modulate.a = mining_away
	if mining_away < 0.1:
		var tween := create_tween()
		tween.tween_property(vignette.material, \"shader_parameter/vignette_intensity\", 50.0, 1.0)
		tween.tween_callback(get_tree().change_scene_to_file.bind(\"res://main.tscn\"))
		


func _on_body_entered(body: Node2D) -> void:
	if body is Player:
		can_collect = true


func _on_body_exited(body: Node2D) -> void:
	if body is Player:
		can_collect = false
"

